{
  "name": "next-ts-firebase-demo",
  "version": "1.0.0",
  "description": "Host Next.js SSR app on Firebase Cloud Functions with Firebase Hosting redirects.",
  "scripts": {
    "dev": "next src/app",
    "serve": "NODE_ENV=production firebase serve --only functions,hosting",
    "deploy": "firebase deploy",
    "clean": "rimraf \"dist/functions\" && rimraf \"dist/public\"",
    "build-app": "next build \"src/app\"",
    "build-public": "cpx \"src/public/**/*.*\" \"dist/public\" -C",
    "build-functions": "tsc --project src/functions",
    "lint-app": "tslint --project src/app",
    "typecheck-app": "tsc --project src/app",
    "lint-functions": "tslint --project src/functions",
    "copy-deps": "cpx \"*{package.json,package-lock.json,yarn.lock}\" \"dist/functions\"",
    "install-deps": "cd \"dist/functions\" && npm i"
  },
  "dependencies": {
    "@material-ui/core": "^3.6.0",
    "@material-ui/icons": "^3.0.1",
    "@types/node": "^10.12.10",
    "@zeit/next-typescript": "^1.1.1",
    "apollo-cache-inmemory": "^1.3.11",
    "apollo-client": "^2.4.7",
    "apollo-link": "^1.2.4",
    "apollo-link-context": "^1.0.10",
    "firebase": "^5.5.8",
    "firebase-admin": "^6.1.0",
    "firebase-functions": "^2.1.0",
    "graphql-tag": "^2.10.0",
    "immutable": "^4.0.0-rc.12",
    "js-cookie": "^2.2.0",
    "next": "^7.0.2",
    "next-apollo": "^2.0.2",
    "react": "^16.6.1",
    "react-apollo": "^2.3.1",
    "react-cookie": "^3.0.5",
    "react-dom": "^16.6.1",
    "react-jss": "^8.6.1",
    "react-redux": "^5.1.1",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "typescript-fsa": "^3.0.0-beta-2",
    "typescript-fsa-reducers": "^1.1.0"
  },
  "devDependencies": {
    "@types/next": "^7.0.4",
    "@types/react": "^16.3.14",
    "cpx": "1.5.0",
    "lint-staged": "^8.0.4",
    "prettier": "1.12.1",
    "rimraf": "2.6.2",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "3.6.0",
    "typescript": "^3.1.6"
  }
}
